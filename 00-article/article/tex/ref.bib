doi = {10.1177/2167702617691560},
file = {:Users/brad/Library/Application Support/Mendeley Desktop/Downloaded/Walsh, Ribeiro, Franklin - 2017 - Predicting Risk of Suicide Attempts Over Time Through Machine Learning.pdf:pdf},
