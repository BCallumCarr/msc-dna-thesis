journal = {Proceedings - 2013 European Intelligence and Security Informatics Conference, EISIC 2013},
keywords = {emotion,machine learning,security informatics,sentiment,social media,text analysis},
pages = {45--51},
title = {{Analyzing social media content for security informatics}},
year = {2011}
}
