issn = {02779536},
journal = {Social Science and Medicine},
keywords = {Celebrity suicide,Imitation,Japan,Media,Social media,Suicide,Twitter,Werther effect},
number = {3},
pages = {19--29},
pmid = {30342383},
publisher = {Elsevier},
title = {{Tracking the Werther Effect on social media: Emotional responses to prominent suicide deaths on twitter and subsequent increases in suicide}},
